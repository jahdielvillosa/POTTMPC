
@{
    ViewBag.Title = "MPV Listing";
}

<div class="jumbotron">
    <h2>MPV Listing</h2>
     <p>
         <a href="/ads/">Listing</a> >
         <a href="#">MPV</a>
     </p>
</div>
<div class="container" >
    <div class="col-lg-8 ads-listing-container">
        <div class="col-sm-12 ads-listing-title-bar">
            <h3>Listing for MPV <span class="badge"> 4 </span></h3>
        </div>

        <div class="col-sm-12 ads-listing-item">

            <h3 class="btn btn-success ads-listing-price">PHP 3,000</h3>
            <img src="~/Images/CarRental/CarViews/ToyotaInnova/Toyota-innova.png" class="img-thumbnail ads-listing-img" />
            <h4>
                <a href="~/ads/toyota-innova-d-4d/">
                    <b>Toyota Innova D-4D</b>
                </a>
            </h4>
            <h4 class="label label-default">Toyota Innova</h4>
            <h4 class="label label-default">MPV</h4>
            <hr style="margin:7px;" />
            <p>Toyota Innova 2013, Milage: 40000, 7 seater, w/ or w/o driver included. </p>
        </div>


        <div class="col-sm-12 ads-listing-item">
            <h3 class="btn btn-success ads-listing-price">PHP 2,000</h3>
            <img src="~/Images/CarRental/CarViews/ToyotaAvanza/ToyotaAvanza.jpg" class="img-thumbnail ads-listing-img" alt="toyota gl grandia 2012" />
            <h4>
                <a href="~/ads/toyota-avanza-1-3e-at/">
                    <b>Cheap Car Rental Davao City, Toyota Avanza 1.3E AT</b>
                </a>
            </h4>
            <h4 class="label label-default">MPV</h4>
            <h4 class="label label-default">Toyota Avanza</h4>
            <hr style="margin:7px;" />
            <p>Car for Rent Davao City. Toyota Avanza 2013. Self drive on Davao City Area. Rental  w/ or w/o driver included. </p>
        </div>


        <div class="col-sm-12 ads-listing-item">
            <h3 class="btn btn-success ads-listing-price">PHP 2,500</h3>
            <img src="~/Images/CarRental/CarViews/ToyotaInnova/ToyotaInnovaSilver.jpg" class="img-thumbnail ads-listing-img" alt="toyota gl grandia" />
            <h4>
                <a href="~/ads/innovacar-for-rent-davao-city/">
                    <b>Innova,Car for Rent Davao City</b>
                </a>
            </h4>
            <h4 class="label label-default">MPV</h4>
            <h4 class="label label-default">Toyota Innova</h4>
            <hr style="margin:7px;" />
            <p>MPV for Rent Davao City. Toyota Innova. Self drive on Davao City Area. Rental  w/ or w/o driver included. </p>
        </div>

        <div class="col-sm-12 ads-listing-item">
            <h3 class="btn btn-success ads-listing-price">PHP 2,000</h3>
            <img src="~/Images/CarRental/CarViews/ToyotaSelfDrive/ToyotaSelfDrive.jpg" class="img-thumbnail ads-listing-img" alt="Innova Self Drive" />
            <h4>
                <a href="~/ads/rent-a-car-davao-city-self-drive/">
                    <b>Rent a car davao city self drive</b>
                </a>
            </h4>
            <h4 class="label label-default">Innova</h4>
            <h4 class="label label-default">MPV</h4>
            <hr style="margin:7px;" />
            <p>MPV for Rent Davao City. Toyota Innova 2013. Self drive on Davao City Area. Rental  w/ or w/o driver included. </p>
        </div>

    </div>
    <div class="col-lg-3 ads-listing-side">
        @{Html.RenderPartial("CarViews/TagsViewListing");}

        @{Html.RenderPartial("CarViews/PopularAdsView");}
    </div>
</div>


