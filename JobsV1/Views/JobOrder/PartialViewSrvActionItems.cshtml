@model IEnumerable<JobsV1.Models.SrvActionItem>
@{ int SvcId = Convert.ToInt32(ViewData["svcId1"]);  }

<div>
    <span>Tasks </span>
        @foreach (var item in Model)
        {
            <span>&#187;</span>
            <span>
                <a id="@item.Id" data_svcid="@SvcId" name="@item.Desc" href="?serviceId=@(SvcId)&sortid=@((int)Session["FilterID"])" class="clickitem">@item.Desc</a>
            </span>
        }
</div>


@Html.Partial("PartialViewJS", null, null)
