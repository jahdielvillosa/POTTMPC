@model JobsV1.Models.CarReservation

@{
    ViewBag.Title = "Details";
}

<br />
<div class="container" >
    <div class="col-lg-12">

        <h1 style="text-align:center">Reservation Details</h1>
        <p  style="text-align:center">Welcome, the following information are your booking details. </p>
    </div>

    <div class="container reservation-container" >
        <h2>Reservation No: @Html.DisplayFor(model => model.Id) </h2>

        <div class="col-sm-5 reservation reservation-right" >
            
                <label> Transaction Date </label>    

                <p>
                    @Html.DisplayFor(model => model.DtTrx)
                </p>

                <label>
                    Start Date
                </label>

                <p>
                    @Html.DisplayFor(model => model.DtStart)
                </p>

                <label>
                   End Date
                </label>

                <p>
                    @Html.DisplayFor(model => model.DtEnd)
                </p>

                <label>
                    Pickup Location
                </label>

                <p>
                    @Html.DisplayFor(model => model.LocStart)
                </p>

                <label>
                   Drop Location
                </label>

                <p>
                    @Html.DisplayFor(model => model.LocEnd)
                </p>


                <label>
                    @Html.DisplayNameFor(model => model.EstHrPerDay)
                </label>

                <p>
                    @Html.DisplayFor(model => model.EstHrPerDay)
                </p>

                <label>
                    @Html.DisplayNameFor(model => model.EstKmTravel)
                </label>

                <p>
                    @Html.DisplayFor(model => model.EstKmTravel)
                </p>

        </div>

        <div class="col-sm-5 reservation" >
            
                <label>
                    Rental Type
                </label>

                <p>
                   @ViewBag.rentalType
                </p>

                <label>
                    @Html.DisplayNameFor(model => model.BaseRate)
                </label>

                <p>
                    @Html.DisplayFor(model => model.BaseRate) PHP
                </p>

                <label>
                    @Html.DisplayNameFor(model => model.Destinations)
                </label>

                <p>
                    @Html.DisplayFor(model => model.Destinations)
                </p>

                <label>
                    @Html.DisplayNameFor(model => model.UseFor)
                </label>

                <p>
                    @Html.DisplayFor(model => model.UseFor)
                </p>

                <label>
                    @Html.DisplayNameFor(model => model.CarUnit.Description)
                </label>

                <p>
                    @Html.DisplayFor(model => model.CarUnit.Description)
                </p>

        </div>
    </div>


    <div class="container reservation-container" >
        <h2>Renter Details</h2>

        <div class="col-lg-5 reservation reservation-right">

            <label>
                @Html.DisplayNameFor(model => model.RenterName)
            </label>

            <p>
                @Html.DisplayFor(model => model.RenterName)
            </p>

            <label>
                @Html.DisplayNameFor(model => model.RenterCompany)
            </label>

            <p>
                @Html.DisplayFor(model => model.RenterCompany)
            </p>

            <label>
                @Html.DisplayNameFor(model => model.RenterEmail)
            </label>

            <p>
                @Html.DisplayFor(model => model.RenterEmail)
            </p>

            <label>
                @Html.DisplayNameFor(model => model.RenterMobile)
            </label>

            <p>
                @Html.DisplayFor(model => model.RenterMobile)
            </p>


        </div>


        <div class="col-sm-5 reservation">

            <label>
                @Html.DisplayNameFor(model => model.RenterAddress)
            </label>

            <p>
                @Html.DisplayFor(model => model.RenterAddress)
            </p>

            <label>
                @Html.DisplayNameFor(model => model.RenterFbAccnt)
            </label>

            <p>
                @Html.DisplayFor(model => model.RenterFbAccnt)
            </p>

            <label>
                @Html.DisplayNameFor(model => model.RenterLinkedInAccnt)
            </label>

            <p>
                @Html.DisplayFor(model => model.RenterLinkedInAccnt)
            </p>

        </div>
    </div>
</div>
