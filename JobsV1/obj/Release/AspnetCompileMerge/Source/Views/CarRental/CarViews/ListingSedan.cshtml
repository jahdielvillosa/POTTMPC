
@{
    ViewBag.Title = "Sedan Listing";
}

<div class="jumbotron">
    <h2>Sedan Listing</h2>
     <p>
         <a href="/ads/">Listing</a> >
         <a href="#">Sedan</a>
     </p>
</div>
<div class="container" >
    <div class="col-lg-8 ads-listing-container">
        <div class="col-sm-12 ads-listing-title-bar">
            <h3>Listing for Sedan <span class="badge"> 4 </span></h3>
        </div>

        <div class="col-sm-12 ads-listing-item">
            <h3 class="btn btn-success ads-listing-price">PHP 2,000</h3>
            <img src="~/Images/CarRental/CarViews/HondaCity/HondaCity2018.png" class="img-thumbnail ads-listing-img" />

            <h3><a href="~/ads/honda-city-automatic/">Honda City 2018</a></h3>
            <h4 class="label label-default">Sedan</h4>
            <h4 class="label label-default">Honda</h4>
            <hr />
            <p>Honda City 22018, Diesel, Milage: 1000, 5 persons driver, w/ or w/o driver included.</p>
            <br />
        </div>


        <div class="col-sm-12 ads-listing-item">
            <h3 class="btn btn-success ads-listing-price">PHP 2,500</h3>
            <img src="~/Images/CarRental/CarViews/SelfDrive/HondaCity2012 (2).jpg" class="img-thumbnail ads-listing-img" alt="honda city 2012" />
            <h4>
                <a href="~/ads/rent-a-car-davao-city-self-drive-2/">
                    <b>Car for rent – Self Drive</b>
                </a>
            </h4>
            <h4 class="label label-default">Sedan</h4>
            <h4 class="label label-default">MPV</h4>
            <h4 class="label label-default">SUV</h4>
            <hr style="margin:7px;" />
            <p>Car for Rent Davao City. Honda City Automatic. Toyota Innova. Self drive on Davao City Area </p>
        </div>

        <div class="col-sm-12 ads-listing-item">
            <h3 class="btn btn-success ads-listing-price">PHP 2,500</h3>
            <img src="~/Images/CarRental/CarViews/ToyotaInnova/Toyota-innova.png" class="img-thumbnail ads-listing-img" alt="honda city 2012" />
            <h4>
                <a href="~/ads/minivan-and-sedan-rentals/">
                    <b>Minivan and Sedan Rentals</b>
                </a>
            </h4>
            <h4 class="label label-default">Sedan</h4>
            <h4 class="label label-default">MPV</h4>
            <h4 class="label label-default">SUV</h4>
            <hr style="margin:7px;" />
            <p>Car for Rent Davao City.Toyota Innova Manual. Self drive on Davao City Area </p>
        </div>

        <div class="col-sm-12 ads-listing-item">
            <h3 class="btn btn-success ads-listing-price">PHP 2,000</h3>
            <img src="~/Images/CarRental/CarViews/HondaCity2012/HondaCityFront.jpg" class="img-thumbnail ads-listing-img" alt="honda city 2012" />
            <h4>
                <a href="~/ads/rent-a-car-davao-city-self-drive/">
                    <b>Davao rent a car (Sedan & MPV)</b>
                </a>
            </h4>
            <h4 class="label label-default">Sedan</h4>
            <h4 class="label label-default">MPV</h4>
            <hr style="margin:7px;" />
            <p>Car for Rent Davao City.Honda City Automatic. Self drive on Davao City Area </p>
        </div>
    </div>
    <div class="col-lg-3 ads-listing-side">
        @{Html.RenderPartial("CarViews/TagsViewListing");}

        @{Html.RenderPartial("CarViews/PopularAdsView");}
    </div>
</div>


